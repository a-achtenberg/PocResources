      <Compile Update="Resources\Common_Resources_Ton_Mobile.Designer.cs">
        <DesignTime>True</DesignTime>
        <AutoGen>True</AutoGen>
        <DependentUpon>Common_Resources_Ton_Mobile.resx</DependentUpon>
      </Compile>
      <Compile Update="Resources\Common_Resources_Stone_Web.Designer.cs">
        <DesignTime>True</DesignTime>
        <AutoGen>True</AutoGen>
        <DependentUpon>Common_Resources_Stone_Web.resx</DependentUpon>
      </Compile>
    </ItemGroup>

</Project>
