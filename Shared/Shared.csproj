<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
    </PropertyGroup>

    <ItemGroup>
      <EmbeddedResource Update="Resources\Common_Resources_Stone.resx">
        <Generator>ResXFileCodeGenerator</Generator>
        <LastGenOutput>Common_Resources_Stone.Designer.cs</LastGenOutput>
      </EmbeddedResource>
      <EmbeddedResource Update="Resources\Common_Resources_Ton_Web.resx">
        <Generator>ResXFileCodeGenerator</Generator>
